{
	"name": "frontend",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "npm run build && wrangler pages dev",
		"build": "ng build && npm run process",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"serve:ssr:frontend": "node dist/frontend/server/server.mjs",
		"process": "node ./tools/copy-files.mjs",
		"deploy": "npm run build && wrangler pages deploy",
		"cf-typegen": "wrangler types"
	},
	"prettier": {
		"printWidth": 100,
		"singleQuote": true,
		"overrides": [
			{
				"files": "*.html",
				"options": {
					"parser": "angular"
				}
			}
		]
	},
	"private": true,
	"packageManager": "npm@11.6.2",
	"dependencies": {
		"@angular/common": "^21.0.0",
		"@angular/compiler": "^21.0.0",
		"@angular/core": "^21.0.0",
		"@angular/forms": "^21.0.0",
		"@angular/platform-browser": "^21.0.0",
		"@angular/platform-server": "^21.0.0",
		"@angular/router": "^21.0.0",
		"@angular/ssr": "^21.0.1",
		"@tanstack/angular-query-experimental": "^5.90.16",
		"rxjs": "~7.8.0",
		"swiper": "^12.0.3",
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@angular/build": "^21.0.1",
		"@angular/cli": "^21.0.1",
		"@angular/compiler-cli": "^21.0.0",
		"@tailwindcss/postcss": "^4.1.12",
		"@types/node": "^20.17.19",
		"jsdom": "^27.1.0",
		"postcss": "^8.5.3",
		"tailwindcss": "^4.1.12",
		"typescript": "~5.9.2",
		"vitest": "^4.0.8",
		"wrangler": "^4.56.0",
		"xhr2": "^0.2.1"
	}
}
